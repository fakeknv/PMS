<UserControl x:Class="PMS.UIComponents.Day"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PMS.UIComponents"
             mc:Ignorable="d" d:DesignWidth="79.5" Height="89.5">
    <Grid>
        <Button Margin="4,4,4,5" Click="Button_Click" Background="{x:Null}"/>
        <Border BorderBrush="#EEEEEE" BorderThickness="1">
            <Grid>
                <Label x:Name="DateVal" Visibility="Hidden"/>
                <Label x:Name="DayHolder" Content="Label" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="41,-1,-1,0" FontFamily="/PMS;component/Assets/Fonts/#Roboto" Foreground="#FF888888"/>
            </Grid>
        </Border>
    </Grid>
</UserControl>
